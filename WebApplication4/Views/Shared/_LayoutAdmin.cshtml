
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Sistema MICRONA | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    

    <link rel="stylesheet" runat="server" href="~/Content/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" runat="server" href="~/Content/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" runat="server" href="~/Content/bower_components/Ionicons/css/ionicons.min.css">
    <link rel="stylesheet" runat="server" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" />
    <!-- Theme style -->
    <link rel="stylesheet" runat="server" href="~/Content/dist/css/AdminLTE.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->    
    <link rel="stylesheet" runat="server" href="~/Content/dist/css/skins/_all-skins.min.css">
    <!-- Morris chart -->
    <link rel="stylesheet" runat="server" href="~/Content/bower_components/morris.js/morris.css">
    <!-- jvectormap -->
    <link rel="stylesheet" runat="server" href="~/Content/bower_components/jvectormap/jquery-jvectormap.css">
    <!-- Date Picker -->
    <link rel="stylesheet" runat="server" type="text/css" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.css">
    <link rel="stylesheet" runat="server" type="text/css" href="~/Content/bootstrap-select/dist/css/bootstrap-select.css"/>

    <link rel="stylesheet" runat="server" href="~/Content/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" runat="server" href="~/Content/bower_components/bootstrap-daterangepicker/daterangepicker.css">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" runat="server" href="~/Content/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">    
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <link href="~/Content/bower_components/select2/dist/css/select2.css" rel="stylesheet" />
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script runat="server" src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script runat="server" src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" runat="server" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-green sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b></b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Microna</b>BETA</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/dist/img/default_user.png" class="user-image" alt="User Image">
                                @{
                                    if (Request.Cookies["userInfo"] != null)
                                    {
                                        <span class="hidden-xs">@Request.Cookies["userInfo"]["nombre"]</span>
                                    }
                                }
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    @{
                                        if (Request.Cookies["userInfo"] != null)
                                        {
                                            <span class="hidden-xs">@Request.Cookies["userInfo"]["tipo"]</span>
                                        }
                                    }
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        @Html.ActionLink("Perfil", "Index", "Perfil", null, new { @class = "btn btn-default btn-flat" })
                                    </div>
                                    <div class="pull-right">
                                        @Html.ActionLink("Cerrar Sesión", "Logout", "Home", null, new { @class = "btn btn-default btn-flat" })
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/Content/dist/img/default_user.png" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        @{
                            if (Request.Cookies["userInfo"] != null)
                            {
                                <p>Usuario: @Request.Cookies["userInfo"]["user"]</p>
                            }
                        }
                    </div>
                </div>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">CONCENTRADO</li>
                    <li>
                        <a href="@Url.Action("Index", "Home", null)">
                            <i class="fa fa-briefcase"></i> <span>Concentrado Total</span>
                        </a>
                    </li>
                    <li class="header">NAVEGACIÓN</li>
                    <li class="treeview">
                        <a href="#">

                            <i class="fa fa-pencil"></i> <span>Artículos</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index", "Articulos", null)"><i class="fa fa-circle-o"></i> Ver</a></li>
                            <li><a href="@Url.Action("Create", "Articulos", null)"><i class="fa fa-circle-o"></i> Añadir</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-files-o"></i>
                            <span>Trabajos Entregados</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index", "Trabajos", null)"><i class="fa fa-circle-o"></i> Ver</a>
                            <li><a href="@Url.Action("Create", "Trabajos", null)"><i class="fa fa-circle-o"></i> Crear</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-book"></i>
                            <span>Libros</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index", "Libro", null)"><i class="fa fa-circle-o"></i> Ver</a>
                            <li><a href="@Url.Action("Create", "Libro", null)"><i class="fa fa-circle-o"></i> Crear</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-book"></i>
                            <span>Capitulo Libro</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index", "CapituloLibro", null)"><i class="fa fa-circle-o"></i> Ver</a>
                            <li><a href="@Url.Action("Create", "CapituloLibro", null)"><i class="fa fa-circle-o"></i> Crear</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-book"></i>
                            <span>Proyectos</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index", "Proyectos", null)"><i class="fa fa-circle-o"></i> Ver</a>
                            <li><a href="@Url.Action("Create", "Proyectos", null)"><i class="fa fa-circle-o"></i> Crear</a></li>
                        </ul>
                    </li>
                    @{
                        if (Request.Cookies["userInfo"] != null)
                        {
                            if (Request.Cookies["userInfo"]["tipo"].Equals("Administrador") || Request.Cookies["userInfo"]["tipo"].Equals("Super-administrador"))
                            {
                                <li class="header">ADMINISTRACIÓN</li>
                                <li>
                                    <a href="@Url.Action("Index", "Usuario")">
                                        <i class='fa fa-users'></i> <span>Usuarios</span>
                                    </a>
                                </li>
                            }
                        }
                    }
                    <li class="header">COMENTARIOS</li>
                    <li>
                        <a href="https://docs.google.com/forms/d/e/1FAIpQLSfDMtzuHu5z6sjVO9MfXmWARKIx3GCY543IdPG8PsBpnIRbag/viewform?usp=sf_link">
                            <i class="fa fa-wechat"></i> <span>Comentarios o sugerencias</span>
                        </a>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <section class="content-header">
                <h1>
                    @ViewBag.Title <small>- @ViewBag.Controlador</small>
                </h1>
            </section>
            <section class="content">
                @RenderBody()
            </section>
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
            </div>
            <strong>Copyright &copy; 2014-2016 <a href="https://adminlte.io">Almsaeed Studio</a>.</strong> All rights
            reserved.
        </footer>

        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    

    <!-- jQuery 3 -->
    <script runat="server" src="~/Content/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script runat="server" src="~/Content/bower_components/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->
    <script runat="server" src="~/Content/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script runat="server" src="~/Content/bower_components/raphael/raphael.min.js"></script>
    <script runat="server" src="~/Content/bower_components/morris.js/morris.min.js"></script>
    <!-- Sparkline -->
    <script runat="server" src="~/Content/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script runat="server" src="~/Content/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script runat="server" src="~/Content/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script runat="server" src="~/Content/bower_components/jquery-knob/dist/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script runat="server" src="~/Content/bower_components/moment/min/moment.min.js"></script>
    <script runat="server" src="~/Content/bower_components/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script runat="server" src="~/Content/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script runat="server" src="~/Content/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script runat="server" src="~/Content/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script runat="server" src="~/Content/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script runat="server" src="~/Content/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script runat="server" src="~/Content/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script runat="server" src="~/Content/dist/js/demo.js"></script>
    <script runat="server" src="//cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script runat="server" src="~/Content/bootstrap-select/dist/js/bootstrap-select.js"></script>
    <script runat="server" src="~/Content/ConfirmationBoostrap/dist/bootstrap-confirmation.js"></script>
    <script runat="server" src="http://bootstrap-confirmation.js.org/dist/bootstrap-confirmation2/bootstrap-confirmation.min.js"></script>
    <script type="text/javascript" language="javascript" src="https://cdn.datatables.net/1.10.18/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" language="javascript" src="https://cdn.datatables.net/buttons/1.5.2/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" language="javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" language="javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js"></script>
    <script type="text/javascript" language="javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js"></script>
    <script type="text/javascript" language="javascript" src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.html5.min.js"></script>
    <script src="~/Content/bower_components/select2/dist/js/select2.js"></script>
    <script src="~/Content/bower_components/select2/dist/js/select2.full.js"></script>    
    <script>
        $(document).ready(function () {
            $(function () {
                $('#datat').DataTable({
                    dom: 'Bfrtip',
                    buttons: [
                        {
                            extend: 'excel',
                            text: 'Guardar excel',
                            className: 'btn btn-warning',
                            exportOptions: {
                                modifier: {
                                    page: 'current'
                                }
                            }
                        },
                        {
                            extend: 'pdfHtml5',
                            text: 'Guardar PDF',
                            className: 'btn btn-warning',
                            exportOptions: {
                                modifier: {
                                    page: 'current'
                                }
                            }
                        }
                    ]
                })
            })
            $(function () {
                $('#basic2').selectpicker({
                    liveSearch: true,
                });
                $('.select2').select2();

            })
            $(function () {
                $('.date-picker').datepicker({
                    beforeShow: function (input, inst) {
                        setTimeout(function () {
                            inst.dpDiv.css({
                                top: $(".datepicker").offset().top + 35,
                                left: $(".datepicker").offset().left
                            });
                        }, 0);
                    },
                    format: "mm-yyyy",
                    startView: "months",
                    minViewMode: "months"
                });
            });
            var update_pizza = function () {
                if ($("#indizado").is(":selected")) {
                    $('#ind').prop('disabled', false);
                }
                else {
                    $('#ind').prop('disabled', 'disabled');
                }
            };
            $(update_pizza);
            $("#tipo_art").change(update_pizza);
            $('[data-toggle=confirmation]').confirmation();
        });
       
    </script>
    <script src="~/Content/site.js"></script>
    <script src="~/Content/TableExcel.js"></script>
</body>
</html>
